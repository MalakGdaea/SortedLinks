{
    "headers": [
        {
            "source": "/assets/:path*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/videos/:path*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/:path*.(png|webp|jpg|jpeg|svg|gif|ico)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=2592000"
                }
            ]
        },
        {
            "source": "/:path*.(css|js)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}